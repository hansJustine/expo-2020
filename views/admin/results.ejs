<%- include('../partials/header') -%>
<%- include('../partials/adminnavbar') -%>
   
    <div class="container">
        <% if(!foundState.state){ %>
            <form action="/adminhub/winners" method="POST">
                <button class="btn btn-primary">Tally Scores</button>
            </form>
        <% } %>
    <div class="container">
            <% for(var i = 0; i < awards.length; i++){ %>
                <div class="row text-center alignCard" style="display: flex; flex-wrap: wrap;">
                    <div class="col-12">
                        <div class="card cardBody" style="width: 100%; height: 28rem;">
                            <div class="card-body">
                                <div id="award<%= i %>"></div>                              
                            </div>
                        </div>
                    </div>
                </div>
            <% } %> 
    </div>




<%- include('../partials/footer') -%>